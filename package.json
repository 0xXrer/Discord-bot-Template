{
  "name": "oceanic-discord-bot",
  "version": "2.0.0",
  "description": "Modern Discord bot template with decorators, DI container, and modular architecture using Oceanic.js",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/main.ts",
    "watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "docker:build": "docker build -t discord-bot .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "oceanic.js": "^1.10.2",
    "tsyringe": "^4.8.0",
    "reflect-metadata": "^0.2.1",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1",
    "@prisma/client": "^5.8.0",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "prom-client": "^15.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "prettier": "^3.1.1",
    "prisma": "^5.8.0"
  },
  "keywords": [
    "discord",
    "bot",
    "oceanic",
    "typescript",
    "decorators",
    "dependency-injection",
    "modular"
  ],
  "author": "0xXrer",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}